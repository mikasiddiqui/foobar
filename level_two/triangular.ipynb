   "source": [
    "def triangular_number(n):\n",
    "    return int((n*(n+1))/2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 33,
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(x, y, num_list):\n",
    "    triangle_row = [triangular_number(n) for n in num_list]\n",
    "    jumps = y-1\n",
    "    index = (x + jumps) - 1\n",
    "    resulting_number = triangle_row[index] - jumps\n",
    "    return resulting_number"
   ]
  },
